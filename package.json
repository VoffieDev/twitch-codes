{
  "name": "twitch-codes",
  "displayName": "Twitch Codes",
  "description": "",
  "version": "0.0.1",
  "publisher": "VoffieDev",
  "keywords": [
    "twitch"
  ],
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VoffieDev/twitch-codes.git"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
